  object zqryPMC: TZQuery
    Connection = dm.Conexao
    SQL.Strings = (
      'SELECT codi_pmc, data_pmc, nume_pmc, TC.valo_com as DescTipo'
      'FROM pmc'
      
        'INNER JOIN tabela_combos TC ON TC.tipo_com = 4 AND TC.codi_com =' +
        ' tipo_pmc'
      'ORDER BY nume_pmc DESC')
    Params = <>
    Left = 304
    Top = 280
  end
  object dspPMC: TDataSetProvider
    DataSet = zqryPMC
    Options = [poAutoRefresh, poAllowCommandText]
    UpdateMode = upWhereKeyOnly
    Left = 344
    Top = 280
  end
