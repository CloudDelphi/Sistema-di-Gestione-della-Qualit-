    UserName = 'frxDBDSMacro'
    CloseDataSource = False
    FieldAliases.Strings = (
      'mac_codigo=mac_codigo'
      'mac_codfator=mac_codfator'
      'mac_codvariavel=mac_codvariavel'
      'mac_tipo=mac_tipo'
      'mac_texto=mac_texto'
      'fator=fator'
      'var_descricao=var_descricao')
    DataSet = cdsImprimir
    BCDToCurrency = False
    Left = 304
    Top = 216
  end
  object zqryImprimir: TZQuery
    Connection = dm.Conexao
    SQL.Strings = (
      
        'SELECT mac_codigo, mac_codfator, mac_codvariavel, mac_tipo, mac_' +
        'texto,'
      'TC.valo_com as Fator, R.var_descricao'
      'FROM risco_macro'
      
        'INNER JOIN tabela_combos TC ON TC.codi_com = mac_codfator and TC' +
        '.tipo_com = 22'
      
        'INNER JOIN risco_variaveis_ext R ON R.var_codigo = mac_codvariav' +
        'el')
    Params = <>
    Left = 360
    Top = 216
  end
